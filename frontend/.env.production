# Environment: production

# URL for the backend API - This is the public domain
REACT_APP_API_URL=https://shpion.pr0d.ru

# URL for the LiveKit server - This is the proxied, secure WebSocket URL
# The nginx proxy will forward requests from /livekit/ to 10.10.3.1:7880
